{
  "name": "@latteswap/latteswap-flat",
  "scripts": {
    "mainnetfork": "source ./start_mainnet_fork_node.sh",
    "copy-dts-typechain": "npx copyfiles -u 1 \"typechain/**/*.d.ts\" compiled-typechain",
    "coverage": "hardhat coverage",
    "build": "tsc -p tsconfig.cjs.json && yarn copy-dts-typechain",
    "compile6": "hardhat typechain --config hardhat.config.6.ts",
    "compile8": "hardhat typechain --config hardhat.config.ts",
    "compile": "hardhat typechain --config hardhat.config.ts && hardhat typechain --config hardhat.config.6.ts",
    "remove:sol-log": "hardhat remove-logs",
    "prettier-format:default": "prettier --config .prettierrc 'deploy/**/*.ts' 'utils/**/*.ts' 'scripts/**/*.ts' 'tests/**/*.ts' --write",
    "deploy:mainnet": "hardhat --network mainnet deploy --reset",
    "test": "hardhat test --no-compile",

    "deploy:mainnet:offchainOracle": "hardhat --network mainnet deploy --no-compile --reset --tags DeployOffChainOracle",
    "deploy:mainnet:chainlinkOracle": "hardhat --network mainnet deploy --no-compile --reset --tags DeployChainlinkOracle",
    "deploy:mainnet:compositeOracle": "hardhat --network mainnet deploy --no-compile --reset --tags DeployCompositeOracle",
    "deploy:mainnet:lpChainlinkAggregator": "hardhat --network mainnet deploy --no-compile --reset --tags DeployLPChainlinkAggregator",
    "deploy:mainnet:FLAT": "hardhat --network mainnet deploy --no-compile --reset --tags DeployFLAT",
    "deploy:mainnet:flatMarketBundle": "hardhat --network mainnet deploy --no-compile --reset --tags DeployFlatMarketBundle",
    "deploy:mainnet:flatMarketConfig": "hardhat --network mainnet deploy --no-compile --reset --tags DeployFlatMarketConfig",
    "deploy:mainnet:clerk": "hardhat --network mainnet deploy --no-compile --reset --tags DeployClerk",
    "deploy:mainnet:latteswapYieldStrategy": "hardhat --network mainnet deploy --no-compile --reset --tags DeployLatteSwapYieldStrategy",
    "deploy:mainnet:pcsYieldStrategy": "hardhat --network mainnet deploy --no-compile --reset --tags DeployPCSYieldStrategy",

    "script:mainnet:oracle:setCompositeOraclePrimarySources": "hardhat --network mainnet run --no-compile ./scripts/oracles/set_composite_oracle_primary_sources.ts",
    "script:mainnet:oracle:getOraclePrices": "hardhat --network mainnet run --no-compile ./scripts/oracles/get_oracle_prices.ts",
    "script:mainnet:flatmarket:setOracles": "hardhat --network mainnet run --no-compile ./scripts/flatmarket/set_oracles.ts",
    "script:mainnet:timelock:upgrade": "hardhat --network mainnet run --no-compile ./scripts/timelock/timelock/timelock_upgrade.ts",
    "script:mainnet:timelock:execute": "hardhat --network mainnet run --no-compile ./scripts/timelock/timelock_execution.ts",
    "script:mainnet:proxy:upgrade": "hardhat --network mainnet run --no-compile ./scripts/proxy/upgrade.ts",
    "script:mainnet:flat:mintAndReplenish": "hardhat --network mainnet run --no-compile ./scripts/flat/mint_and_replenish.ts",

    "test:integrations:clerk": "hardhat test --no-compile ./tests/integration/Clerk.test.ts",
    "test:integrations:flatMarket": "hardhat test --no-compile ./tests/integrations/FlatMarket.test.ts",
    "test:integrations:flatMarketConfig": "hardhat test --no-compile ./tests/integrations/FlatMarketConfig.test.ts",
    "test:integrations:latteSwapLiquidateStrategy": "hardhat test --no-compile ./tests/integrations/LatteSwapLiquidateStrategy.test.ts",
    "test:unit:flat": "hardhat test --no-compile ./tests/unit/Flat.test.ts",
    "test:unit:flatMarket": "hardhat test --no-compile ./tests/unit/FlatMarket.test.ts",
    "test:unit:tokenChainlinkAggregator": "hardhat test --no-compile ./tests/unit/TokenChainlinkAggregator.test.ts",
    "test:unit:lpChainlinkAggregator": "hardhat test --no-compile ./tests/unit/LPChainlinkAggregator.test.ts",
    "test:unit:latteSwapYieldStrategy": "hardhat test --no-compile ./tests/unit/LatteSwapYieldStrategy.test.ts",
    "test:unit:PCSYieldStrategy": "hardhat test --no-compile ./tests/unit/PCSYieldStrategy.test.ts",
    "test:unit:clerk": "hardhat test --no-compile ./tests/unit/clerk.test.ts",
    "test:unit:compositeOracle": "hardhat test --no-compile ./tests/unit/CompositeOracle.test.ts",
    "test:unit:offChainOracle": "hardhat test --no-compile ./tests/unit/OffChainOracle.test.ts",
    "test:unit:treasuryHolder": "hardhat test --no-compile ./tests/unit/TreasuryHolder.test.ts",
    "mainnet": "hardhat console --network mainnet",
    "test:case": "mocha --timeout 500000 --require hardhat/register --file tests/**/* --extension ts -g",
    "prepublishOnly": "yarn compile && yarn build"
  },
  "version": "0.1.0-rc.1",
  "main": "index.js",
  "license": "GPL-3.0",
  "devDependencies": {
    "@eth-optimism/smock": "^1.1.10",
    "@ethersproject/bignumber": "^5.0.14",
    "@latteswap/latteswap-contract-config": "^1.6.0-rc.5",
    "@nomiclabs/ethereumjs-vm": "^4.2.2",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.2",
    "@openzeppelin/contracts-upgradeable": "^4.3.2",
    "@openzeppelin/hardhat-upgrades": "^1.9.0",
    "@typechain/ethers-v5": "7.0.1",
    "@typechain/hardhat": "2.0.2",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.0",
    "chai-bn": "^0.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "ethereum-waffle": "^3.2.2",
    "ethereumjs-util": "^7.0.10",
    "ethers": "^5.0.0",
    "fp-ts": "^2.11.4",
    "hardhat": "2.5.0",
    "hardhat-deploy": "^0.7.0-beta.45",
    "hardhat-log-remover": "^2.0.2",
    "hardhat-typechain": "^0.3.5",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "5.1.1",
    "typescript": "^4.1.5",
    "@defi-wonderland/smock": "^2.0.7",
    "@latteswap/latteswap-contract": "^2.2.0",
    "@sushiswap/core": "^1.4.2",
    "solidity-coverage": "^0.7.17"
  },
  "files": [
    "typechain",
    "artifacts",
    "contracts",
    "compiled-typechain"
  ]
}
